{
    "name": "react-file-trecker",
    "displayName": "React File Trecker",
    "description": "Find and track React imports in your workspace",
    "version": "0.0.2",
    "engines": {
        "vscode": "^1.80.0"
    },
    "activationEvents": [
        "onCommand:react-file-trecker.findImports"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "react-file-trecker.findImports",
                "title": "Find React Imports"
            },
            {
                "command": "react-file-trecker.openFile",
                "title": "Open File from Tracker"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "reactFileTreckerSidebar",
                    "title": "React Tracker",
                    "icon": "media/icon.svg"
                }
            ]
        },
        "views": {
            "reactFileTreckerSidebar": [
                {
                    "id": "reactFileTreckerView",
                    "name": "Tracker Panel"
                }
            ]
        }
    }
}